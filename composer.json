{
    "name": "osdvf/tableintogsheet",
    "description": "TableIntoGSheet - mirror your SQL table into Google Sheets",
    "type": "library",
    "homepage": "https://github.com/OSDVF/TableIntoGSheet",
    "authors": [
        {
            "name": "OSDVF",
            "email": "o.s.dv.f@seznam.cz"
        }
    ],
    "require": {
        "google/apiclient": "2.12.1"
    },
    "autoload": {
        "files": ["copy.php"]
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Sheets"
        ]
    }
}